{
  "name": "skullking",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docker": "cd docker; docker-compose up --build -d; yarn web",
    "web": "docker exec -it node bash",
    "docker-stop": "cd docker; docker-compose down",
    "dev": "bash ./dev.sh",
    "client-build": "next build ./client",
    "server-build": "rm -rf dist; tsc -p server",
    "build": "yarn server-build; yarn client-build",
    "start": "NODE_ENV=production node ./dist/index.js",
    "lint": "next lint",
    "test": "jest",
    "open-ca": "open ./docker/nginx/skullkingCA/ca.der"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.10",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "express": "^4.17.1",
    "framer-motion": "^4",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.18.0",
    "recoil": "^0.4.1",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/react": "17.0.24",
    "@types/recoil": "^0.0.9",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "esbuild": "^0.13.9",
    "esbuild-jest": "^0.5.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-tailwindcss": "^1.16.0",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "serve": "^12.0.1",
    "ts-node": "^10.3.0",
    "typescript": "4.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
